'arraylib' crequire! as array

def :assert = { ? 'pass' : 'fail' }

'checking array create'
(1 2 3 array.from-stack! as a # 0 = assert!)
a/# 3 = assert!

'checking array/to-stack'
(a/to-stack # 3 = assert!)

'checking array get/index'
0 a! 1 = assert!
1 a! 2 = assert!
2 a! 3 = assert!

'checking array/set'
10 1 a/set
1 a! 10 = assert!

'checking array/swap'
0 2 a/swap
0 a! 3 = assert!
2 a! 1 = assert!

'checking array/insert'
9 1 a/insert
1 a! 9 = assert!
2 a! 10 = assert!

'checking array/erase'
0 a/erase
0 a! 9 = assert!

'checking array/append'
(5 6 7 a/append)
a/# 6 = assert!

5 a! 7 = assert!


