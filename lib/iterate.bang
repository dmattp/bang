def :times totalTimes = { fun! loopFun =
    def :innerLoop remain = {
      loopFun!
      fun= remain 1 - innerLoop!; remain 1 >?
    }
    totalTimes innerLoop!
}


def :range = { as last as first as do-me
   def :inner current = {
      current do-me!
      fun = current 1 + inner!; current last <?
   }
   first inner!
}

fun = lookup

