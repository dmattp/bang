def :times totalTimes = { fun! loopFun =
    def :innerLoop remain = {
      loopFun!
      remain 1 >
      ? remain 1 - innerLoop!
    }
    totalTimes innerLoop!
}


def :range last = { as first as do-me
   def :inner current = {
      current do-me!
      current last <
      ? current 1 + inner!
   }
   first inner!
}

fun = lookup

